{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "lib": ["es2016"],
    "jsx": "react",
    "jsxFactory": "jsx",
    "declaration": true,
    "sourceMap": true,
    "stripInternal": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true
  }
}
