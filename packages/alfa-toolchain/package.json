{
  "$schema": "http://json.schemastore.org/package",
  "name": "@siteimprove/alfa-toolchain",
  "homepage": "https://alfa.siteimprove.com",
  "version": "0.66.0",
  "license": "MIT",
  "description": "Toolchain for developing Alfa and related projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/siteimprove/alfa.git",
    "directory": "packages/alfa-toolchain"
  },
  "bugs": "https://github.com/siteimprove/alfa/issues",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./changelog": "./src/changelog.js",
    "./changelog.js": "./src/changelog.js"
  },
  "types": "src/index.d.ts",
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@svitejs/changesets-changelog-github-compact": "^1.1.0"
  },
  "devDependencies": {
    "@changesets/types": "^5.2.1"
  }
}
