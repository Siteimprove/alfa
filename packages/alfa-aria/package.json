{
  "$schema": "http://json.schemastore.org/package",
  "name": "@siteimprove/alfa-aria",
  "homepage": "https://alfa.siteimprove.com",
  "version": "0.104.1",
  "license": "MIT",
  "description": "Functionality for working with ARIA and the accessibility tree",
