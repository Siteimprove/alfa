    "@siteimprove/alfa-set": "workspace:^",
    "@siteimprove/alfa-string": "workspace:^",
    "@siteimprove/alfa-style": "workspace:^",
    "@siteimprove/alfa-table": "workspace:^",
    "@siteimprove/alfa-thunk": "workspace:^",
    "@siteimprove/alfa-tree": "workspace:^"
