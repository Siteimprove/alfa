{
  "private": true,
  "name": "@siteimprove/alfa-cli",
  "author": "Siteimprove A/S",
  "homepage": "https://siteimprove.com",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Suite of open and standards-based tools for performing reliable accessibility conformance testing at scale",
  "keywords": [
    "a11y",
    "accessibility",
    "aria",
    "typescript",
    "wcag"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/siteimprove/alfa.git",
    "directory": "packages/alfa-cli"
  },
  "bugs": "https://github.com/siteimprove/alfa/issues",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "alfa": "bin/alfa.js"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "bin/**/*.js",
    "bin/**/*.d.ts"
  ],
  "oclif": {
    "commands": "./bin/alfa/commands",
    "bin": "alfa",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@siteimprove/alfa-act": "^0.0.0",
    "@siteimprove/alfa-device": "^0.0.0",
    "@siteimprove/alfa-json-ld": "^0.0.0",
    "@siteimprove/alfa-rules": "^0.0.0",
    "@siteimprove/alfa-scraper": "^0.0.0",
    "@siteimprove/alfa-web": "^0.0.0",
    "enquirer": "^2.3.0"
  },
  "devDependencies": {
    "@siteimprove/alfa-test": "^0.0.0"
  }
}
